{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.png",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.jpg",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/Login/index.html"
    },
    {
      "src": "/login",
      "dest": "/Login/index.html"
    },
    {
      "src": "/Menu/indexM.html",
      "dest": "../Menu/indexM.html"
    },
    {
      "src": "/menu",
      "dest": "../Menu/indexM.html"
    },
    {
      "src": "/ranking",
      "dest": "../Ranking/indexR.html"
    },
    {
      "src": "/amigos", 
      "dest": "../Amigos/indexA.html"
    },
    {
      "src": "/desafios",
      "dest": "../Desafios/indexD.html"
    },
    {
      "src": "/provas",
      "dest": "../Provas/indexPr.html"
    },
    {
      "src": "/configuracao",
      "dest": "../Configuração/indexC.html"
    },
    {
      "src": "/perfil",
      "dest": "../Perfil/indexP.html"
    },
    {
      "src": "/contato",
      "dest": "../Contato/indexCo.html"
    },
    {
      "src": "/navegador",
      "dest": "../Navegador/indexN.html"
    },
    {
      "src": "/(.*)",
      "dest": "/Login/index.html"
    }
  ]
}

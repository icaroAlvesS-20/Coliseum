{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/menu",
      "dest": "/Menu/indexM.html"
    },
    {
      "src": "/ranking",
      "dest": "/Ranking/indexR.html"
    },
    {
      "src": "/amigos",
      "dest": "/Amigos/indexA.html"
    },
    {
      "src": "/desafios",
      "dest": "/Desafios/indexD.html"
    },
    {
      "src": "/provas",
      "dest": "/Provas/indexPr.html"
    },
    {
      "src": "/configuracao",
      "dest": "/Configuração/indexC.html"
    },
    {
      "src": "/perfil",
      "dest": "/Perfil/indexP.html"
    },
    {
      "src": "/contato",
      "dest": "/Contato/indexCo.html"
    },
    {
      "src": "/navegador",
      "dest": "/Navegador/indexN.html"
    },
    {
      "src": "/login",
      "dest": "/Login/index.html"
    },
    {
      "src": "/estudo",
      "dest": "/Estudo/indexEs.html"
    },
    {
      "src": "/saeb",
      "dest": "/EstudoSaeb/index.html"
    },
    {
      "src": "/prova-brasil",
      "dest": "/EstudoPB/index.html"
    },
    {
      "src": "/saresp",
      "dest": "/Estudosaresp/index.html"
    },
    {
      "src": "/etec",
      "dest": "/EstudoEtec/index.html"
    },
    {
      "src": "/matematica",
      "dest": "/Matemática/indexMT.html"
    },
    {
      "src": "/portugues",
      "dest": "/LinguaPortuguesa/indexLP.html"
    },
    {
      "src": "/ciencias-humanas",
      "dest": "/CSH/indexCSH.html"
    },
    {
      "src": "/ciencias-natureza",
      "dest": "/CSN/indexCSN.html"
    },
    {
      "src": "/",
      "dest": "/Login/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/Login/index.html"
    }
  ]
}

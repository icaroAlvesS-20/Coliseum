{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.png",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.jpg",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/Login/index.html"
    },
    {
      "src": "/login",
      "dest": "/Login/index.html"
    },
    {
      "src": "/Menu",
      "dest": "../Menu/indexM.html"
    },
    {
      "src": "/menu",
      "dest": "/Menu/indexM.html"
    },
    {
      "src": "/Ranking",
      "dest": "/Ranking/indexR.html"
    },
    {
      "src": "/ranking",
      "dest": "/Ranking/indexR.html"
    },
    {
      "src": "/Amigos",
      "dest": "/Amigos/indexA.html"
    },
    {
      "src": "/Desafios", 
      "dest": "/Desafios/indexD.html"
    },
    {
      "src": "/Provas",
      "dest": "/Provas/indexPr.html"
    },
    {
      "src": "/Configuracao",
      "dest": "/Configuração/indexC.html"
    },
    {
      "src": "/Perfil",
      "dest": "/Perfil/indexP.html"
    },
    {
      "src": "/Contato",
      "dest": "/Contato/indexCo.html"
    },
    {
      "src": "/Navegador",
      "dest": "/Navegador/indexN.html"
    },
    {
      "src": "/(.*)",
      "dest": "/Login/index.html"
    }
  ]
}

{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/menu",
      "dest": "/Menu/indexM.html"
    },
    {
      "src": "/ranking",
      "dest": "/Ranking/indexR.html"
    },
    {
      "src": "/amigos",
      "dest": "/Amigos/indexA.html"
    },
    {
      "src": "/desafios",
      "dest": "/Desafios/indexD.html"
    },
    {
      "src": "/provas",
      "dest": "/Provas/indexPr.html"
    },
    {
      "src": "/configuracao",
      "dest": "/Configuracao/indexC.html"
    },
    {
      "src": "/perfil",
      "dest": "/Perfil/indexP.html"
    },
    {
      "src": "/contato",
      "dest": "/Contato/indexCo.html"
    },
    {
      "src": "/navegador",
      "dest": "/Navegador/indexN.html"
    },
    {
      "src": "/login",
      "dest": "/Login/index.html"
    },
    {
      "src": "/estudo",
      "dest": "/Estudo/indexEs.html"
    },
    {
      "src": "/saeb",
      "dest": "/EstudoSaeb/indexSB.html"
    },
    {
      "src": "/prova-brasil",
      "dest": "/EstudoPB/indexPB.html"
    },
    {
      "src": "/saresp",
      "dest": "/EstudoSaresp/indexSR.html"
    },
    {
      "src": "/etec",
      "dest": "/EstudoEtec/indexPE.html"
    },
    {
      "src": "/",
      "dest": "/Login/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/Login/index.html"
    }
  ]
}
